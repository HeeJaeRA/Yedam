package ch1;

public class Exam01 {

	public static void main(String[] args) {
		// TODO 자동 생성된 메소드 스텁
		
		char c1 = 'a';
		int c2 = c1 + 1;
		System.out.println(c2);
		
		int x = 5;
		int y = 2;
		double result = x / y;
		System.out.println(result);
		
		double result1 = (double) x / y;
		System.out.println(result1);
		
		double var1 = 3.5;
		double var2 = 2.7;
		int result2 = (int) var1 + (int) var2;
		System.out.println(result2);
	}

}
