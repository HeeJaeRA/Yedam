package ch1;

public class Exam02 {

	public static void main(String[] args) {
		// TODO 자동 생성된 메소드 스텁
		
		String name = "감자바";
		int age = 25;
		String tel1 = "010", tel2 = "123", tel3 = "4567";
		System.out.println("이름: " + name);
		System.out.print("나이: " + age + "\n");
		System.out.printf("전화: %s-%s-%s", tel1, tel2, tel3);

	}

}
